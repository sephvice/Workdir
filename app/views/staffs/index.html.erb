<p id="notice"><%= notice %></p>

<div class="head">
      <div>
         <% if staff_signed_in? %>
         <ul>
           <li>

               <%= link_to current_staff.email, staff_path(current_staff) %>
           </li>
           <li>
             <%= link_to "Edit Account", edit_staff_registration_path %>
           </li>
           <li>
             <%= link_to "Sign Out", destroy_staff_session_path, method: :delete %>
           </li>
         </ul>

      </div>
</div>

<h1>User Dashboard</h1>

<section>
  <div>
      <%= link_to 'Create new Post', new_feed_path %>
    <ul>
    <% @feeds.each do |feed| %>
      <li><%= feed.content %></li>
    <% end %>
  </ul>
  </div>
</section>
<% else %>
<div>
  <ul>
   <li>
     <%= link_to "Sign In", staff_session_path %>
    </li>
    <li>
      <%= link_to "Sign Up", new_staff_registration_path %>
    </li>
  </ul>
</div>
  <% end %>
